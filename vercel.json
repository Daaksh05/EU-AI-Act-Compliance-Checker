{
  "outputDirectory": "dist",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  },
  "redirects": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "rewrites": [
    {
      "source": "/api/check",
      "destination": "/api/check"
    },
    {
      "source": "/api/download/:report_id",
      "destination": "/api/download/:report_id"
    }
  ]
}